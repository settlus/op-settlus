{
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 120,
    "plugins": ["prettier-plugin-solidity"],
    "overrides": [
        {
          "files": "*.sol",
          "options": {
            "bracketSpacing": false,
            "explicitTypes": "always"
          }
        }
      ]
}